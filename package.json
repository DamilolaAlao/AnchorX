{
  "name": "AnchorX",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.8",
    "@storybook/addon-links": "^3.4.8",
    "@storybook/addons": "^3.4.8",
    "@storybook/react-native": "^3.4.8",
    "@types/jest": "^22.2.3",
    "@types/react": "^16.3.14",
    "@types/react-native": "^0.55.14",
    "@types/react-test-renderer": "^16.0.1",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "jest-expo": "~27.0.0",
    "prop-types": "^15.6.2",
    "react-dom": "16.3.1",
    "react-native-scripts-ts": "1.15.0",
    "react-native-typescript-transformer": "^1.2.3",
    "react-test-renderer": "16.3.1",
    "ts-jest": "^22.4.6",
    "tslib": "^1.9.1",
    "typescript": "^2.8.3"
  },
  "main": "./node_modules/react-native-scripts-ts/build/bin/crna-entry.js",
  "scripts": {
    "start": "react-native-scripts-ts start",
    "eject": "react-native-scripts-ts eject",
    "android": "react-native-scripts-ts android",
    "ios": "react-native-scripts-ts ios",
    "test": "jest",
    "storybook": "storybook start -p 7007"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "useBabelrc": true
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "preset": "jest-expo",
    "testMatch": [
      "**/__tests__/**/*.[tj]s?(x)",
      "**/?(*.)(spec|test).[tj]s?(x)"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!native-base-shoutem-theme|react-native-easy-grid|react-native|react-navigation|native-base/node_modules/react-native-vector-icons)"
    ]
  },
  "dependencies": {
    "@types/react-navigation": "^2.0.10",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "expo": "^27.0.1",
    "native-base": "^2.7.1",
    "react": "16.3.1",
    "react-native": "~0.55.2",
    "react-native-style-tachyons": "^3.7.0",
    "react-navigation": "^2.6.2"
  }
}
